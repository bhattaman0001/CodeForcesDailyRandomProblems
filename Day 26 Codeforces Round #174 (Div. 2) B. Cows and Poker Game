// https://codeforces.com/contest/284/problem/B
#include <bits/stdc++.h>
#define int long long int
using namespace std;
void solve()
{
    int n;
    cin >> n;
    int a = 0, b = 0;
    for (int i = 0; i < n; i++)
    {
        char ch;
        cin >> ch;
        if (ch == 'A')
          a++;
        if (ch == 'I')
          b++; 
    }
    if(b>1)
        cout<<"0\n";
    else if(b==1)
        cout<<"1\n";
    else
        cout<<a<<'\n';    
    return;
}

signed main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    solve();
}
