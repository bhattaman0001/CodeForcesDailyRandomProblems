ll gcd(ll a, ll b) { return b == 0 ? a : gcd(b, a % b); }
ll lcm(ll a, ll b) { return (a / gcd(a, b)) * b; }
using namespace std;

// ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥Author : Aman Bhatt (Codeforces handle: bhattaman0001)ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥

void solve()
{
    cout << "\n";
    int k, n, m;
    cin >> k >> n >> m;
    int a[n], b[m], c[n + m], gg = 0;
    for (int &x : a)
        cin >> x;
    for (int &x : b)
        cin >> x;
    merge(a, a + n, b, b + m, c);
    for (int x : c)
    {
        if (x == 0)
            k += 1;
        else
            gg |= x > k;
    }
    if (gg)
        cout << "-1\n";
    else
    {
        for (int i : c)
            cout << i << " ";
    }
    cout << endl;
    return;
}

// Main Begin
int main()
{
    f fs;
    int test;
    cin >> test;
    cout << "\n";
    while (test--)
    {
        solve();
    }
    return 0;
}
